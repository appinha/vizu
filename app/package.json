{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "tsc -b ; eslint .",
    "lint:fix": "tsc -b ; eslint . --fix ; npm run format",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "vitest",
    "test:ui": "vitest --ui",
